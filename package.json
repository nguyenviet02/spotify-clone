{
  "name": "spotify-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "codegen": "graphql-codegen --config codegen.yaml"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "@emotion/react": "^11.12.0",
    "@emotion/styled": "^11.12.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "dotenv": "^16.4.5",
    "next": "14.2.4",
    "notistack": "^3.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-split": "^2.0.14",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix --cache"
    ],
    "*.{json,js,ts,jsx,tsx,css}": [
      "prettier --write --ignore-unknown"
    ]
  }
